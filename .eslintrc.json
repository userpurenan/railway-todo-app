{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest"
    ],
    "rules": {
        "react/prop-types": "off"
    },
    "settings": {
        "react": {
          "version": "detect"
        }
      }
}
